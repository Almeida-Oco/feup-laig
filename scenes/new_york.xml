<SCENE>

	<!-- Initial values -->
	<INITIALS>
		<frustum near="0.1" far="500"/>
		<translation x="-1" y="1" z="0"/>
		<rotation axis="x" angle="0" />
		<rotation axis="y" angle="0" />
		<rotation axis="z" angle="0" />
		<scale sx="0.25" sy="0.25" sz="0.25" />
		<reference length="5" />
	</INITIALS>

	<!-- Illumination and clear color -->
	<ILLUMINATION>
		<ambient r="0.2" g="0.2" b="0.2" a="1" />
		<background r="0" g="0" b="0" a="1" />
	</ILLUMINATION>

	<!-- Lights -->
	<LIGHTS>
		<LIGHT id="light0">
			<enable value ="1" />
			<position  x="0" y="10" z="0" w="1" />
			<ambient r="1.0" g="1.0" b="1.0" a="1.0" />
			<diffuse r="1.0" g="1.0" b="1.0" a="1.0" />
			<specular r="1.0" g="1.0" b="1.0" a="1.0" />
		</LIGHT>
	</LIGHTS>

	<TEXTURES>
		<!-- Rocks -->
		<TEXTURE id="rocks">
			<file path="images/rocks.jpg" />
			<amplif_factor s="1" t="1" />
		</TEXTURE>

		<!-- New york floor bench -->
		<TEXTURE id="nwfloor">
			<file path="images/nwfloor.jpg" />
			<amplif_factor s="1" t="1" />
		</TEXTURE>

	</TEXTURES>

	<MATERIALS>
		<MATERIAL id="piramide">
			<shininess value="1" />
			<specular r="0" g="0" b="0" a="1" />
			<diffuse r="1.0" g="0.68" b="0.4" a="1" />
			<ambient r="0.2" g="0.2" b="0.2" a="1" />
			<emission r="0.0" g="0.0" b="0.0" a="1" />
		</MATERIAL>
	</MATERIALS>

	<!-- The nodes -->
	<NODES>
		<ROOT id="root" />

		<NODE id="root">
			<MATERIAL id="piramide"/>
			<TEXTURE id="rocks"/>

			<DESCENDANTS>
				<NODEREF id="helices"/>
				<NODEREF id="corpo"/>
				<NODEREF id="towers" />
				<NODEREF id="chao" />
			</DESCENDANTS>
		</NODE>

		<NODE id="helices" >
			<MATERIAL id="piramide"/>
			<TEXTURE id="rocks"/>

			<ROTATION axis="y" angle="180" />
			<SCALE sx="1" sy="1" sz="1" />

			<DESCENDANTS>
				<NODEREF id="topo"/>
				<NODEREF id="cauda"/>
			</DESCENDANTS>
		</NODE>

		<NODE id="topo" >
			<MATERIAL id="piramide"/>
			<TEXTURE id="rocks"/>

			<DESCENDANTS>
				<NODEREF id="hel1"/>
				<NODEREF id="hel2"/>
				<NODEREF id="hel3"/>
				<NODEREF id="hel4"/>
			</DESCENDANTS>
		</NODE>

		<NODE id="hel1">
			<MATERIAL id="piramide"/>
			<TEXTURE id="rocks"/>

			<DESCENDANTS>
				<LEAF type="rectangle" args="10 0 12 6"/>
			</DESCENDANTS>
		</NODE>

		<NODE id="hel2">
			<MATERIAL id="piramide"/>
			<TEXTURE id="rocks"/>

			<ROTATION axis="z" angle="90"/>

			<DESCENDANTS>
				<LEAF type="rectangle" args="10 0 12 6"/>
			</DESCENDANTS>
		</NODE>

		<NODE id="hel3">
			<MATERIAL id="piramide"/>
			<TEXTURE id="rocks"/>

			<ROTATION axis="z" angle="-90"/>

			<DESCENDANTS>
				<LEAF type="rectangle" args="10 0 12 6"/>
			</DESCENDANTS>
		</NODE>

		<NODE id="hel4">
			<MATERIAL id="piramide"/>
			<TEXTURE id="rocks"/>

			<ROTATION axis="z" angle="180"/>

			<DESCENDANTS>
				<LEAF type="rectangle" args="10 0 12 6"/>
			</DESCENDANTS>
		</NODE>

		<NODE id="cauda" >
			<MATERIAL id="piramide"/>
			<TEXTURE id="rocks"/>

			<DESCENDANTS>
				<LEAF type="patch" orderU="8" orderV="6" partsU="20" partsV="20" >
					<controlpoint x="0" y="0" z="-5.5"/>
					<controlpoint x="0" y="-0.5" z="-5"/>
					<controlpoint x="0" y="-3" z="-4"/>
					<controlpoint x="0" y="-3" z="0"/>
					<controlpoint x="0" y="-3" z="4"/>
					<controlpoint x="0" y="-0.5" z="5"/>
					<controlpoint x="0" y="0" z="4.8"/>

					<controlpoint x="0" y="0" z="-5.5"/>
					<controlpoint x="0.353" y="-0.353" z="-5"/>
					<controlpoint x="2.12" y="-2.12" z="-4"/>
					<controlpoint x="2.12" y="-2.12" z="0"/>
					<controlpoint x="2.12" y="-2.12" z="4"/>
					<controlpoint x="0.353" y="-0.353" z="5"/>
					<controlpoint x="0" y="0" z="4.8"/>

					<controlpoint x="0" y="0" z="-5.5"/>
					<controlpoint x="0.5" y="0" z="-5"/>
					<controlpoint x="3" y="0" z="-4"/>
					<controlpoint x="3" y="0" z="0"/>
					<controlpoint x="3" y="0" z="4"/>
					<controlpoint x="0.5" y="0" z="5"/>
					<controlpoint x="0" y="0" z="4.8"/>

					<controlpoint x="0" y="0" z="-5.5"/>
					<controlpoint x="0.353" y="0.353" z="-5"/>
					<controlpoint x="2.12" y="2.12" z="-4"/>
					<controlpoint x="2.12" y="2.12" z="0"/>
					<controlpoint x="2.12" y="2.12" z="4"/>
					<controlpoint x="0.353" y="0.353" z="5"/>
					<controlpoint x="0" y="0" z="4.8"/>

					<controlpoint x="0" y="0" z="-5.5"/>
					<controlpoint x="0" y="0.5" z="-5"/>
					<controlpoint x="0" y="3" z="-4"/>
					<controlpoint x="0" y="3" z="0"/>
					<controlpoint x="0" y="3" z="4"/>
					<controlpoint x="0" y="0.5" z="5"/>
					<controlpoint x="0" y="0" z="4.8"/>

					<controlpoint x="0" y="0" z="-5.5"/>
					<controlpoint x="-0.353" y="0.353" z="-5"/>
					<controlpoint x="-2.12" y="2.12" z="-4"/>
					<controlpoint x="-2.12" y="2.12" z="0"/>
					<controlpoint x="-2.12" y="2.12" z="4"/>
					<controlpoint x="-0.353" y="0.353" z="5"/>
					<controlpoint x="0" y="0" z="4.8"/>

					<controlpoint x="0" y="0" z="-5.5"/>
					<controlpoint x="-0.5" y="0" z="-5"/>
					<controlpoint x="-3" y="0" z="-4"/>
					<controlpoint x="-3" y="0" z="0"/>
					<controlpoint x="-3" y="0" z="4"/>
					<controlpoint x="-0.5" y="0" z="5"/>
					<controlpoint x="0" y="0" z="4.8"/>

					<controlpoint x="0" y="0" z="-5.5"/>
					<controlpoint x="-0.353" y="-0.353" z="-5"/>
					<controlpoint x="-2.12" y="-2.12" z="-4"/>
					<controlpoint x="-2.12" y="-2.12" z="0"/>
					<controlpoint x="-2.12" y="-2.12" z="4"/>
					<controlpoint x="-0.353" y="-0.353" z="5"/>
					<controlpoint x="0" y="0" z="4.8"/>

					<controlpoint x="0" y="0" z="-5.5"/>
					<controlpoint x="0" y="-0.5" z="-5"/>
					<controlpoint x="0" y="-3" z="-4"/>
					<controlpoint x="0" y="-3" z="0"/>
					<controlpoint x="0" y="-3" z="4"/>
					<controlpoint x="0" y="-0.5" z="5"/>
					<controlpoint x="0" y="0" z="4.8"/>
				</LEAF>
			</DESCENDANTS>
		</NODE>

		<NODE id="corpo" >
			<MATERIAL id="piramide"/>
			<TEXTURE id="rocks"/>

			<DESCENDANTS>
				<LEAF type="rectangle" args="2 4 2 4" />
			</DESCENDANTS>
		</NODE>





		<!-- The floor -->
		<NODE id="chao">
			<MATERIAL id="null" />
			<TEXTURE id="nwfloor" />

			<SCALE sx="40" sy="1" sz="85" />
			<TRANSLATION x="0" y="-33.5" z="0" />
			<ROTATION axis="x" angle="-90" />
			<TRANSLATION x="-2.5" y="-1.5" z="0" />

			<DESCENDANTS>
				<LEAF  type="rectangle" args="0 4 5 0" />
			</DESCENDANTS>

		</NODE>




		<!-- TWIN TOWERS PRE 2001 -->
		<NODE id="towers">
			<MATERIAL id="piramide" />
			<TEXTURE id="rocks" />
			<SCALE sx="3" sy="3" sz="3" />
			<ROTATION axis="z" angle="-90" />
			<ROTATION axis="x" angle="90" />
			<TRANSLATION x="0" y="-10" z="0" />

			<DESCENDANTS>
				<NODEREF id="tower1" />
				<NODEREF id="tower2" />
			</DESCENDANTS>

		</NODE>

		<NODE id="tower1">
			<MATERIAL id="null" />
			<TEXTURE id="null" />

			<DESCENDANTS>
				<NODEREF id="tower" />
			</DESCENDANTS>

		</NODE>

		<NODE id="tower2">
			<MATERIAL id="null" />
			<TEXTURE id="null" />

			<TRANSLATION x="0" y="0" z="3.5" />

			<DESCENDANTS>
				<NODEREF id="tower" />
			</DESCENDANTS>

		</NODE>

		<!-- Generic tower -->
		<NODE id="tower">
			<MATERIAL id="null" />
			<TEXTURE id="null" />

			<ROTATION axis="y" angle="90" />
			<SCALE sx="0.5" sy="0.5" sz="1.5" />

			<DESCENDANTS>

				<NODEREF id="faceCubo1" />
				<NODEREF id="faceCubo2" />
				<NODEREF id="faceCubo3" />
				<NODEREF id="faceCubo4" />
				<NODEREF id="faceCubo5" />
				<NODEREF id="faceCubo6" />
			</DESCENDANTS>

		</NODE>

		<!-- A cube face -->
		<NODE id="faceCubo1">
			<MATERIAL id="null" />
			<TEXTURE id="null" />

			<TRANSLATION x="2.5" y="4" z="7.5" />

			<DESCENDANTS>
				<LEAF  type="rectangle" args="0 4 5 0" />
			</DESCENDANTS>

		</NODE>

		<!-- Another cube face -->
		<NODE id="faceCubo2">
			<MATERIAL id="null" />
			<TEXTURE id="null" />

			<TRANSLATION x="2.5" y="4" z="2.5" />
			<ROTATION axis="y" angle="-90" />

			<DESCENDANTS>
				<LEAF  type="rectangle" args="0 4 5 0" />
			</DESCENDANTS>

		</NODE>

		<!-- Another cube face -->
		<NODE id="faceCubo3">
			<MATERIAL id="null" />
			<TEXTURE id="null" />

			<TRANSLATION x="7.5" y="4" z="7.5" />
			<ROTATION axis="y" angle="90" />

			<DESCENDANTS>
				<LEAF  type="rectangle" args="0 4 5 0" />
			</DESCENDANTS>

		</NODE>

		<!-- Another cube face -->
		<NODE id="faceCubo4">
			<MATERIAL id="null" />
			<TEXTURE id="null" />

			<TRANSLATION x="7.5" y="4" z="2.5" />
			<ROTATION axis="y" angle="180" />

			<DESCENDANTS>
				<LEAF  type="rectangle" args="0 4 5 0" />
			</DESCENDANTS>

		</NODE>

		<!-- Another cube face -->
		<NODE id="faceCubo5">
			<MATERIAL id="null" />
			<TEXTURE id="null" />

			<ROTATION axis="x" angle="-90" />
			<TRANSLATION x="2.5" y="-7.5" z="8" />
			<SCALE sx="1" sy="1.25" sz="1" />

			<DESCENDANTS>
				<LEAF  type="rectangle" args="0 4 5 0" />
			</DESCENDANTS>

		</NODE>

		<!-- Another cube face -->
		<NODE id="faceCubo6">
			<MATERIAL id="null" />
			<TEXTURE id="null" />

			<TRANSLATION x="2.5" y="4" z="2.5" />
			<ROTATION axis="x" angle="90" />
			<SCALE sx="1" sy="1.25" sz="1" />

			<DESCENDANTS>
				<LEAF  type="rectangle" args="0 4 5 0" />
			</DESCENDANTS>

		</NODE>






	</NODES>
</SCENE>
