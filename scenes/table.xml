        <ambient r="0.2" g="0.2" b="0.2" a="1.0" />
        <diffuse r="0.6" g="0.6" b="0.6" a="1.0" />
        <specular r="0.6" g="0.6" b="0.6" a="1.0" />
    </LIGHT>

</LIGHTS>

<!-- Textures -->
<TEXTURES>
    <TEXTURE id="light_wood_text">
        <file path="images/light_wood.jpg" />
        <amplif_factor s="2" t="2" />
    </TEXTURE>

		<TEXTURE id="dark_wood_text">
			<file path="images/dark_wood.jpg" />
			<amplif_factor s="0.2" t="0.2" />
		</TEXTURE>

</TEXTURES>


<MATERIALS>
	<MATERIAL id="wood_mat">
        <shininess value="1" />
        <specular r="0.1" g="0.1" b="0.05" a="1" />
        <diffuse r="0.8" g="0.8" b="0.6" a="1" />
        <ambient r="0.5" g="0.5" b="0.5" a="1" />
        <emission r="0.0" g="0.0" b="0.0" a="1" />
    </MATERIAL>
</MATERIALS>

<ANIMATIONS>
	<ANIMATION id="linear1" speed="10" type="linear">
		<controlpoint xx="0" yy="0" zz="0" />
		<controlpoint xx="5" yy="0" zz="5" />
		<controlpoint xx="10" yy="0" zz="0" />
	</ANIMATION>
</ANIMATIONS>


