			<emission r="0" g="0" b="0" a="1"/>
		</MATERIAL>

		<MATERIAL id="pads_mat">
			<shininess value="20"/>
			<specular r="0.7" g="0.7" b="0.8" a="1"/>
			<diffuse r="0.6" g="0.6" b="0.7" a="1" />
			<ambient r="0.1" g="0.1" b="0.1" a="1"/>
