		<ANIMATION id="helLinear" speed ="10" type="linear">
			<controlpoint xx="0" yy="0" zz="0" />
			<controlpoint xx="0" yy="0" zz="5" />
			<controlpoint xx="5" yy="6" zz="12" />
		</ANIMATION>

		<ANIMATION id="helRot" speed="10" type="circular"
							 centerx="5" centery="6" centerz="0"
							 radius="10" startang="0" rotang="-122"/>

		<ANIMATION id="helLinear2" speed="8" type="linear">
			<controlpoint xx="0" yy="0" zz="0" />
			<controlpoint xx="0" yy="2" zz="20" />
			<controlpoint xx="-10" yy="-2" zz="30" />
			<controlpoint xx="0" yy="20" zz="0" />
		</ANIMATION>

		<ANIMATION id="helicesRot" speed="0.1" type="circular"
							 centerx="0" centery="3.5" centerz="4"
							 radius="0.01" startang="0" rotang="90" loop="true" />


		<ANIMATION id="earthRot" speed="0.005" type="circular"
								centerx="-1" centery="1" centerz="-11"
								radius="0.01" startang="0" rotang="90" loop="true" />


		<ANIMATION id="zepBezier1" speed="2" type="bezier">
			<controlpoint xx="10" yy="20" zz="10" />
			<controlpoint xx="15" yy="22" zz="10" />
			<controlpoint xx="15" yy="25" zz="10" />
			<controlpoint xx="15" yy="35" zz="10" />
		</ANIMATION>


		<ANIMATION id="zepCirc" speed="0.3" type="circular"
							 centerx="15" centery="35" centerz="10"
							 radius="0.7" startang="0" rotang="-115" loop="false" />


		<ANIMATION id="zepBezier2" speed="2" type="bezier">
			<controlpoint xx="15" yy="35" zz="10" />
			<controlpoint xx="11" yy="31" zz="15" />
			<controlpoint xx="11" yy="31" zz="25" />
			<controlpoint xx="14" yy="31" zz="30" />
		</ANIMATION>


		<ANIMATION id="zepCombo" type="combo">
			<SPANREF id ="zepBezier1" />
			<SPANREF id ="zepCirc" />
			<SPANREF id ="zepBezier2" />
		</ANIMATION>



	</ANIMATIONS>

