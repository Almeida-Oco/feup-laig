<SCENE>

	<!-- Initial values -->
	<INITIALS>
		<frustum near="0.1" far="500"/>
		<translation x="0" y="0" z="-4"/>
		<rotation axis="x" angle="0" />
		<rotation axis="y" angle="0" />
		<rotation axis="z" angle="0" />
		<scale sx="0.25" sy="0.25" sz="0.25" />
		<reference length="5" />
	</INITIALS>

	<!-- Illumination and clear color -->
	<ILLUMINATION>
		<ambient r="0.1" g="0.1" b="0.1" a="1" />
		<background r="0" g="0.55" b="0.55" a="1" />
	</ILLUMINATION>

	<!-- Lights -->
	<LIGHTS>
		<LIGHT id="light0">
			<enable value ="1" />
			<position  x="0" y="10" z="0" w="1" />
			<ambient r="1.0" g="1.0" b="1.0" a="1.0" />
			<diffuse r="1.0" g="1.0" b="1.0" a="1.0" />
			<specular r="1.0" g="1.0" b="1.0" a="1.0" />
		</LIGHT>

		<LIGHT id="light1">
			<enable value ="1" />
			<position  x="0" y="35" z="-20" w="1" />
			<ambient r="1.0" g="1.0" b="1.0" a="1.0" />
			<diffuse r="1.0" g="1.0" b="1.0" a="1.0" />
			<specular r="1.0" g="1.0" b="1.0" a="1.0" />
		</LIGHT>


		<LIGHT id="sunLight">
			<enable value ="1" />
			<position  x="-1" y="3" z="-5" w="1" />
			<ambient r="1.0" g="0.5" b="0" a="1.0" />
			<diffuse r="1.0" g="0.5" b="0" a="1.0" />
			<specular r="1.0" g="0.5" b="0" a="1.0" />
		</LIGHT>

	</LIGHTS>

	<TEXTURES>
		<TEXTURE id="marble">
			<file path="images/marble_tex.jpg" />
			<amplif_factor s="1" t="1" />
		</TEXTURE>

		<!-- New york floor -->
		<TEXTURE id="grass">
			<file path="images/grass.jpg" />
			<amplif_factor s="1" t="1" />
		</TEXTURE>

		<TEXTURE id="goodyear">
			<file path="images/goodyear.png" />
			<amplif_factor s="1" t="1" />
		</TEXTURE>

		<TEXTURE id="red">
			<file path="images/red.jpg"/>
			<amplif_factor s="1" t="1"/>
		</TEXTURE>

		<TEXTURE id="hel_metal">
			<file path="images/camouflage.jpg"/>
			<amplif_factor s="1" t="1"/>
		</TEXTURE>

		<TEXTURE id="grey_metal">
			<file path="images/black_plastic.jpg"/>
			<amplif_factor s="1" t="1"/>
		</TEXTURE>

		<TEXTURE id="black_metal">
			<file path="images/b_metal.jpg"/>
			<amplif_factor s="1" t="1"/>
		</TEXTURE>

		<TEXTURE id="earth">
			<file path="images/earth.jpeg"/>
			<amplif_factor s="1" t="1"/>
		</TEXTURE>

		<TEXTURE id="pillar_text">
			<file path="images/pillar.jpg"/>
			<amplif_factor s="1" t="1"/>
		</TEXTURE>

		<TEXTURE id="lid__marble">
